{% extends 'base.html.twig' %}

{% block title %}Taldea{% endblock %}

{% block body %}
    <h1>Taldea</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ taldea.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ taldea.name }}</td>
            </tr>
            <tr>
                <th>Enabled</th>
                <td>{{ taldea.enabled ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Created</th>
                <td>{{ taldea.created ? taldea.created|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Updated</th>
                <td>{{ taldea.updated ? taldea.updated|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('taldea_index') }}">back to list</a>

    <a href="{{ path('taldea_edit', {'id': taldea.id}) }}">edit</a>

    {{ include('taldea/_delete_form.html.twig') }}
{% endblock %}