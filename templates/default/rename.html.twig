{{ form_start(form) }}
<div class="modal-content">
    <div class="modal-content">
        <div class="modal-header">
            <h5 class="modal-title" id="exampleModalLabel">{{ "app.crud.rename" | trans }}</h5>
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
        </div>
        <div class="modal-body">
            {{ form_row(form.name) }}
        </div>
        <div class="modal-footer">
            <button type="button" class="btn btn-secondary" data-dismiss="modal">{{ "app.crud.close" | trans }}</button>
            <button type="submit" class="btn btn-sm btn-primary">{{ "app.crud.save" | trans }}</button>

        </div>
    </div>
    <div style="display: none;">
        {{ form_widget(form.curdir) }}
        {{ form_rest(form) }}
    </div>
    {{ form_end(form) }}
</div>